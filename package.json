{
  "name": "sfa-get",
  "version": "0.7.0",
  "description": "SFA-Get retrieves story formats from the Story Format Archive",
  "main": "index.ts",
  "bin": {
    "sfa-get": "built/src/index.js"
  },
  "scripts": {
    "lint": "npx eslint ./src/*.ts --fix",
    "lint:test": "npx eslint ./tests/*.test.ts --fix",
    "build": "tsc",
    "test": "jest --coverage --silent",
    "start": "node built/src/index.js",
    "all": "npm run lint && npm run lint:test && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/videlais/story-formats-archive.git"
  },
  "keywords": [
    "twine2",
    "story",
    "format",
    "twine1"
  ],
  "author": "Dan Cox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/videlais/story-formats-archive/issues"
  },
  "homepage": "https://github.com/videlais/story-formats-archive#readme",
  "dependencies": {
    "@inquirer/prompts": "^7.4.0",
    "axios": "^1.8.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-plugin-jest": "^28.11.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  },
  "type": "module"
}
